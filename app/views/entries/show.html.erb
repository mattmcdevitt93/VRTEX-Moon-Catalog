<%= will_paginate @report, class: "float-left" %>

 <table>
  <thead>
    <tr>
   	  <th>ID</th>
      <th>User</th>
      <th>Entry</th>
      <th>System</th>
      <th>System esi</th>
      <th>Planet</th>
      <th>Planet esi</th>
      <th>Moon</th>
      <th>Moon esi</th>
      <th>Product</th>
      <th>Product</th>
      <th>Percent</th>
      <th>Status</th>
      <th>Verified</th>
      <th>Flag</th>
    </tr>
  </thead>

  <tbody>
    <% @report.each do |catalog| %>
      <tr>
        <td><%= catalog.id %></td>
        <td><%= catalog.user_id %></td>
        <td><%= catalog.entry_id %></td>
        <td><%= catalog.system_id %></td>
        <td><%= catalog.system_esi %></td>
        <td><%= catalog.planet_id %></td>
        <td><%= catalog.planet_esi %></td>
        <td><%= catalog.moon_id %></td>
        <td><%= catalog.moon_esi %></td>
        <td><%= catalog.product_id %></td>
        <td><%= catalog.product %></td>
        <td><%= catalog.percent %></td>
        <td><%= catalog.status %></td>
        <td><%= catalog.verified %></td>
        <td><%= catalog.flag %></td>
      </tr>
    <% end %>
  </tbody>
</table>
